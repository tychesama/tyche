{
  "name": "tyche",
  "private": true,
  "workspaces": [
    "apps/home-page",
    "apps/portfolio-website",
    "apps/blog",
    "shared/*"
  ],
  "scripts": {
    "dev:home": "npm --workspace apps/home-page run dev",
    "dev:portfolio": "npm --workspace apps/portfolio-website run dev",
    "dev:blog": "npm --workspace apps/blog run dev",
    "dev:all": "concurrently \"npm run dev:home\" \"npm run dev:portfolio\" \"npm run dev:blog\""
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "next": "^16.1.6",
    "react-icons": "^5.5.0",
    "tailwind-scrollbar-hide": "^4.0.0"
  }
}
